/**index.wxss**/

.page {
    background-color: #f1f1f1;
    position: relative;
    /* padding-bottom: 200rpx; */
    width: 750rpx;
    padding-top: 82rpx;
    /* height: 115vh; */
}

swiper {
    width: 100vw;
    height: 60vw;
    background-color: #adfadf;
}

swiper image {
    width: 100%;
    height: 100%;
}

view.head {
    padding: 10rpx;
    position: fixed;
    width: 750rpx;
    height: 100rpx;
    box-sizing: border-box;
    top: 0;
    z-index: 50;
    background-color: #2655B6;
    overflow: hidden;
    display: flex;
    justify-content: center;
    align-items: center;
}

view.head input {
    /* border: 1px solid #eee; */
    border-radius: 18rpx;
    background-color: rgba(255, 255, 255, .3);
    /* border: 1px solid #eee; */
    width: 70%;
    border-radius: 50rpx;
    font-size: 32rpx;
    padding-left: 80rpx;
    color: #fff;
    height: 82rpx;
    line-height: 41rpx;
}

.class-img {
    width: 60rpx;
    height: 60rpx;
    margin-right: 10rpx;
}

.sea-img {
    position: absolute;
    width: 60rpx;
    height: 60rpx;
    left: 130rpx;
    z-index: 20;
}

view.head .input-placeholder {
    font-size: 32rpx;
    color: #fff;
}

navigator#search {
    position: absolute;
    display: block;
    width: 80vw;
    left: 10vw;
    top: 8vw;
    height: 32px;
    background-color: rgba(200, 200, 255, 0.3);
    border-radius: 16px;
    box-shadow: 1px 1px 3px rgba(0, 0, 0, 0.6) inset;
    z-index: 100;
    background-image: url(data:image/svg+xml;base64,);
    background-size: 17px 17px;
    background-repeat: no-repeat;
    background-position: 10px 8px;
}

navigator#search::before {
    content: "搜索商品";
    color: #fff;
    position: relative;
    top: 5px;
    left: 32px;
    text-shadow: 1px 1px 0 #999;
}

view#nav {
    width: 100vw;
    display: table;
    background-color: #f9f9f9;
}

view#nav>navigator {
    width: 33.3%;
    display: table-cell;
    padding: 18px 0;
}

view#nav>navigator>image {
    width: 48px;
    height: 48px;
    display: block;
    margin: 0 auto;
}

view#nav>navigator>text {
    text-align: center;
    display: block;
    color: #999;
}

view#h2 {
    font-size: 20px;
    font-weight: 400;
    padding: 12px 0 8px 0;
    color: #888;
    text-align: center;
    width: 100%;
    background-image: url(data:image/svg+xml;base64,);
    background-repeat: no-repeat;
    background-position: center center;
}

view.products {
    border-top: 1px solid #eee;
    min-height: 60px;
}

view.products>navigator:first-of-type {
    background-color: #f9f9f9;
    padding: 6px 12px 8px 12px;
    border-top: 1px solid #fff;
    font-size: 30rpx;
    font-weight: bold;
    position: relative;
    background-image: linear-gradient(to top, #eee 0, #f9f9f9 6px);
    text-shadow: 1px 1px #fff;
    color: #666;
    overflow: hidden;
    display: block;
}

view.products>navigator:first-of-type::before {
    content: "更多 »";
    display: inline-block;
    width: 80rpx;
    height: 50rpx;
    color: #bbb;
    position: absolute;
    right: 20rpx;
    top: 10rpx;
    bottom: 50rpx;
    font-size: 22rpx;
}

view.products>navigator {
    box-sizing: border-box;
}

.pro-list {
    display: flex;
    box-sizing: border-box;
    width: 750rpx;
    padding: 6rpx;
    justify-content: flex-start;
    flex-wrap: wrap;
}

.nav {
    /* border: 1px solid red; */
    background-color: #fff;
    width: 360rpx;
    border-radius: 5rpx;
    box-shadow: 1px 1px 1px #f7f7f7;
    margin-left: 10rpx;
    margin-top: 10rpx;
}

view.products>navigator:first-of-type::after {
    content: "";
    width: 0;
    height: 0;
    position: absolute;
    left: 24px;
    top: 30px;
    border-top: none;
    border-left: 8px solid transparent;
    border-right: 8px solid transparent;
    border-bottom: 8px solid #fff;
}

view.products>navigator:nth-of-type(2n+2) {
    display: inline-block;
    width: calc(50vw + 1px);
    border-right: 1px solid #eee;
    border-bottom: 1px solid #eee;
    margin: 0;
}

view.products>navigator:nth-of-type(2n+3) {
    display: inline-block;
    width: calc(50vw - 1px);
    border-bottom: 1px solid #eee;
    margin: 0;
}

view.products>navigator:nth-of-type(n+2):nth-last-of-type(1) {
    border-bottom: none;
}

view.products>navigator:nth-of-type(2n+2):nth-last-of-type(2) {
    border-bottom: none;
}

view.products>navigator:nth-of-type(n+2) {
    margin-bottom: -4px;
    padding: 12px 10px;
    height: 274px;
    vertical-align: top;
}

view.product_image {
    width: 100%;
    height: 250rpx;
}

.img {
    /* height: 100rpx; */
}

view.product_image>image {
    width: 100%;
    height: 100%;
    /* height: 100rpx; */
    /* background-color: #f6f6f6 */
}

view.product_thumb {
    border-top: 1px solid #eee;
    padding-top: 10px;
    box-sizing: border-box;
    height: 75rpx;
    width: 100%;
    overflow: hidden;
    clear: both;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.prc-sale {
    /* border: 1px solid red; */
    width: 100%;
    display: flex;
    justify-content: space-around;
    align-items: center;
}

view.product_thumb>image {
    width: 40px;
    height: 40px;
    display: block;
    float: left;
    margin-left: 20rpx;
    background-color: #f6f6f6;
    background-clip: content-box;
}

view.product_thumb>image:nth-of-type(n+2) {
    margin-left: 4px;
}

text.product_price {
    color: #FB2104;
    font-size: 34rpx;
    font-weight: bold;
}

text.text-price {
    font-size: 25rpx;
}

text.product_sales {
    font-size: 28rpx;
    float: right;
    color: #999;
    margin-top: 4px;
}

view.product_title {
    width: 100%;
    padding: 0 10rpx;
    font-size: 25rpx;
    font-weight: bold;
    color: #5f5f5f;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    height: 24px;
    padding-top: 10px;
}

view.product_shop {
    width: 100%;
    padding: 5rpx 10rpx 20rpx 10rpx;
    font-size: 25rpx;
    color: #666;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

view.product_shop::before {
    content: "";
    width: 14px;
    height: 12px;
    display: inline-block;
    background-image: url(data:image/svg+xml;base64,);
    background-repeat: no-repeat;
    background-size: 8px 12px;
    position: relative;
    left: 0;
    top: 2px;
}

#footer {
    /* position: absolute;
    bottom: 0; */
    background-color: #e2e2e2;
    margin-top: 10rpx;
    color: #5f5f5f;
    width: 100vw;
    overflow: hidden;
    height: 120rpx;
    /* background-image: linear-gradient(to bottom, rgb(219, 216, 216) 0px, #999 72px); */
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

#footer>text {
    display: block;
    text-align: center;
    font-size: 25rpx;
    white-space: nowrap;
}


